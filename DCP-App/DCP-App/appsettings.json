{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
      //{
      //  "Name": "File",
      //  "Args": {
      //    "path": ".\\Logs\\log.txt",
      //    "rollingInterval": "Day"
      //  }
      //}
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "DCP"
    }
  },
  "ClientId": "1", // The unique client id for the application, used for identifieing each DCP's, and is used in the topic nanme
  "IsTurbine": true,
  "MqttConsumer": {
    "Host": "127.0.0.1",
    "Port": 1883,
    //"ClientId": "DCP-Consumer",
    //"Username": "DCP-Consumer",
    //"Password": "DCP-Consumer",
    "ConcurrentProcesses": 2
  },
  "MqttProvider": {
    "Enabled": true,
    "Host": "127.0.0.1",
    "Port": 1883,
    //"ClientId": "DCP-Provider",
    //"Username": "DCP-Provider",
    //"Password": "DCP-Provider",
    "ConcurrentProcesses": 2,
    "PublishDataAvailableSeconds": 5
  },
  "InfluxDB": {
    "Host": "http://localhost:8086",
    "Token": "e705baaad1ce982017ff29ef2592536a800f9e29bdc5014d179a92db1a53d306",
    "Bucket": "OMA",
    "Org": "OMA"
  }
